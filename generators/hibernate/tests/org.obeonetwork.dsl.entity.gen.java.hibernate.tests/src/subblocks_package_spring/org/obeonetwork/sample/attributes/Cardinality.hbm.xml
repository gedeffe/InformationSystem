<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">

    <class 
    	name="org.obeonetwork.sample.attributes.Cardinality" 
    	table="CARDINALITY"
		dynamic-update="true">

		<!-- Technical ID -->
        <id name="id" type="string" unsaved-value="null" column="ID">
            <generator class="uuid"/>
        </id>

		<!-- Properties -->
        <property name="one">
            <column name="ONE"/>
        </property>
        <property name="zeroOrOne">
            <column name="ZERO_OR_ONE"/>
        </property>


		<!-- Associations -->


		<!-- Inheritance -->


    </class>

</hibernate-mapping>
