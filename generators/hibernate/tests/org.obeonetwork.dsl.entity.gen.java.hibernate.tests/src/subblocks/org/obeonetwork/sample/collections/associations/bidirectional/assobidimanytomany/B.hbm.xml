<hibernate-mapping auto-import="false">

    <class 
    	name="org.obeonetwork.sample.collections.associations.bidirectional.assobidimanytomany.B" 
    	table="B_7"
		dynamic-update="true">

		<!-- Technical ID -->
        <id name="id" type="string" unsaved-value="null" column="ID">
            <generator class="uuid"/>
        </id>

		<!-- Properties -->


		<!-- Associations -->

		<!--  ___             ___
			 | B |- * {-} * -| A |
